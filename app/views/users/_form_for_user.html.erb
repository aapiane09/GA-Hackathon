<%= form_for @user do |f| %>
  <div class="row">
    <div class="col-md-6">
      <%= f.label :first_name, class:"input_field_label" %>:
      <%= f.text_field :first_name, placeholder: "First Name", id:"form" %>
    </div>
    <div class="col-md-6">
      <%= f.label :last_name, class:"input_field_label" %>:
      <%= f.text_field :last_name, placeholder: "Last Name", id:"form" %>
    </div>
  </div>
  <div class="row">
    <div class="col-md-6">
      <%= f.label :username, class:"input_field_label" %>:
      <%= f.email_field :username, placeholder: "Username" %>
    </div>
    <div class="col-md-6">
      <%= f.label :email, class:"input_field_label" %>:
      <%= f.email_field :email, placeholder: "Email" %>
    </div>
  </div>
  <div class="row">
    <div class="col-md-6">
      <%= f.label :social_url, class:"input_field_label" %>:
      <%= f.text_field :social_url, placeholder: "Social" %>
    </div>
    <div class="col-md-6">
      <%= f.label :course, class:"input_field_label" %>:
      <%= f.text_field :course, placeholder: "Course" %>
    </div>
  </div>
  <div class="row">
    <%= f.label :attachments, class:"input_field_label" %>:
    <%= f.file_field :photo %>
  </div>
  <%= f.submit "Save", class:"btn btn-danger" %>
<% end %>
